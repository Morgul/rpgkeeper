uded. The signs are drawn in blood, and the seals are broken. Your destiny beckons.","initialHP":15,"hpPerLevel":5},"race":"Half-Elf","size":"Medium","level":8,"gender":"Male","alignment":"Good","languages":[],"paragonPath":"-","epicDestiny":"-","strength":8,"constitution":14,"dexterity":10,"intelligence":18,"wisdom":12,"charisma":20,"initiativeFeat":0,"initiativeMisc":0,"speed":6,"armorAbility":"intelligence","armorBonus":2,"armorShieldBonus":0,"armorEnh":2,"armorMisc":0,"fortClassBonus":0,"fortEnh":2,"fortMisc":0,"refClassBonus":1,"refShieldBonus":0,"refEnh":2,"refMisc":0,"willClassBonus":1,"willEnh":2,"willMisc":0,"miscHitPoints":0,"curHitPoints":29,"tmpHitPoints":0,"surgesPerDay":8,"currentSurges":8,"secondWindAvailable":true,"actionPoints":1,"experience":0,"copper":0,"silver":0,"gold":1185,"platinum":0},"u2n7dyb04pqnwfeu9xu9g1qjatq60zc3":{"baseChar":"u2n7dyb04pqnwfeu9xu9g1qjatq60zc3","conditions":[],"skills":[{"$id":"mbxc8emt30tq6kdxqgpgr6qnz9bepgr7"},{"$id":"9e45mtu9crvjrmngm25zpapmv3qp26vz"},{"$id":"wmpt8zwcu6qjcn6dwdrytfr3wqj4wfrf"},{"$id":"kbnrtxkc4qzb9ewnuw6uwjqce6vj1uuz"},{"$id":"pkp32rhr7krgd1kcqbrh2k49bkbxphfp"},{"$id":"wjy410uvbh3e0nf37yaaaazkpaa0m2cu"},{"$id":"erq9hch7htd5ctufbd9t5vfzwe2q5er9"},{"$id":"mga8qv9wd8ywf3qfb6151nw62dyc6xjp"},{"$id":"248a57nxm8bu85feaqgraybrwh05y2bu"},{"$id":"c8tv1t53292un752kfhy66uxkhwm8ayw"},{"$id":"2m16wxgnycu752athzpta5vx3am64zte"},{"$id":"q7dm1kvm13uc6xz01jayzv51fzj6rp4v"},{"$id":"u86fw5054ja8hwr8u3ra01aa5pb6181x"},{"$id":"p7zdbgftaz9819d310qrr78kkcw5xguj"},{"$id":"j7emjfy4mt3uubud6ypqpx4p4t6b71hk"},{"$id":"14etmw14tp14acn30yyfwp9cg4u4zrkf"},{"$id":"cjf0x83wqgxf5gmt86w3ze61q46b0451"}],"powers":[{"$id":"uxt2ppekzhvpte7ypfaca3gaqfgux3dk"},{"$id":"n9m63amqa58p61cu4jgf73h6urczxmbr"},{"$id":"hmye8fev79x9gx5mw7f3paqte5qkgbnm"},{"$id":"140u6x89wnga6w01carb06u7p8bnkmu4"},{"$id":"gevu0c9e97dd98zemaa4j00hdx5yydyq"},{"$id":"k9bgeczgajj4cggbtp7a925wmnxmvend"},{"$id":"j6u4088mwwc72mxddzcvraehve02uu2v"},{"$id":"tp4fjcxwn2hwd9m45gktp