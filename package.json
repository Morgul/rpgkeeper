    "@vue/component-compiler-utils": "^2.3.1",
    "autoprefixer": "^9.4.3",
    "cpr": "^3.0.1",
    "parcel": "^1.11.0",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "socket.io-client": "^2.0.3",
    "vue": "^2.4.4",
    "vue-code": "^1.2.3",
