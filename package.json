{
  "name": "rpgkeeper",
  "version": "2.0.0-beta.3",
  "description": "A site for managing your digital character sheets.",
  "main": "server.js",
  "scripts": {
    "start": "DEBUG=true node ./server.js",
    "dist-clean": "rimraf dist/*; rimraf .cache",
    "clean": "rimraf dist/*",
    "copy": "cpr client/static/ dist/static",
    "build": "npm run clean && npm run copy && parcel build client/index.html --detailed-report",
    "watch": "npm run clean && npm run copy && parcel watch client/index.html"
  },
  "repository": "Morgul/rpgkkeper",
  "author": "Christopher S. Case <chris.case@g33xnexus.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "base62": "^2.0.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.15.2",
    "codemirror": "^5.25.0",
    "colors": "^1.3.3",
    "connect-session-knex": "^1.4.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "fastmatter": "^2.0.1",
    "gravatar": "^1.3.1",
    "knex": "^0.16.3",
    "lodash": "^4.15.0",
    "lru-cache": "^5.1.1",
    "mailgun": "^0.5.0",
    "marked": "^0.5.2",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "passport": "^0.4.0",
    "passport-google-web": "^1.0.0",
    "rpgdicejs": "^1.0.0",
    "socket.io": "^2.0.3",
    "sqlite3": "^4.0.4",
    "thinky": "^2.3.9",
    "trivial-logging": "^1.3.0",
    "trivialperms": "^1.0.0-rc.3",
    "uuid": "^3.1.0"
