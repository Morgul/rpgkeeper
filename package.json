{
  "name": "rpgkeeper",
  "version": "2.0.0-beta.4",
  "description": "A site for managing your digital character sheets.",
  "main": "server.js",
  "scripts": {
    "start": "DEBUG=true node ./server.js",
    "build:cache-clean": "rimraf dist/* .cache",
    "build:clean": "rimraf dist/*",
    "build:copy": "cpr client/static/ dist/static",
    "build:release": "npm run build:cache-clean && npm run build:copy && parcel build client/index.html --detailed-report",
    "build:watch": "npm run build:clean && npm run build:copy && parcel watch client/index.html",
    "dev:theme": "parcel serve client/bootstrap-demo.html"
  },
